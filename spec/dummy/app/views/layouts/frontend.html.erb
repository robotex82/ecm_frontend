<!DOCTYPE html>
<html>
<head>
  <title>Frontend</title>
  <%= render 'frontend/stylesheets' %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div class="container">
    <section id="header">
      <%= render 'frontend/header' %>
    </section>
    <nav id="main-navigation">
      <%= render 'frontend/main_navigation' %>
    </nav>  
    <section id="content">
      <section id="content-container">
        <%= render 'frontend/flash' %>
        <%= yield %>
      </section>
    </section> 
    <section id="footer">
      <%= render 'frontend/footer' %>
    </section>
  </div> 
</body>
</html>
